<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#1DE9B6"
    tools:context=".OneFragment">



    <ImageView
        android:id="@+id/mhee"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_centerInParent="true"
        android:src="@drawable/mhee"/>
    <TextView
        android:id="@+id/name3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pakpoom Paobunprung"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/mhee"
        android:textSize="30sp"/>
    <Button
        android:id="@+id/btnClickFragThree"
        android:layout_below="@+id/name3"
        android:layout_width="170dp"
        android:layout_height="40dp"
        android:layout_centerHorizontal="true"
        android:background="#FFAB91"
        android:textColor="#F37E0606"
        android:text="GO TO MY FRAGMENT"/>


</RelativeLayout>